  Eprint                   = {1201.1299},
  File                     = {2012MNRAS.421.2109B.pdf:2012MNRAS.421.2109B.pdf:PDF},
  Keywords                 = {galaxies: evolution, galaxies: interactions, galaxies: irregular, galaxies: kinematics and dynamics, Magellanic Clouds},
  Owner                    = {magda},
  Primaryclass             = {astro-ph.GA},
  Timestamp                = {2014.07.17}
